{
  "name": "tagged-string",
  "version": "0.0.0",
  "description": "Lightweight, zero-dependency TypeScript library for extracting structured entity information from strings using tag-based syntax",
  "license": "MIT",
  "author": "tbeseda",
  "type": "module",
  "main": "./src/TaggedStringParser.ts",
  "types": "./src/types.ts",
  "exports": {
    ".": "./src/TaggedStringParser.ts",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "lint": "biome check --write",
    "test": "node --test src/**/*.test.ts",
    "posttest": "npm run lint",
    "examples": "node --experimental-strip-types src/examples.ts"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@types/node": "^24.10.0",
    "typescript": "^5.7.2"
  }
}
